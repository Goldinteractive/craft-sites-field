{
    "name": "goldinteractive/sites-field",
    "type": "craft-plugin",
    "license": "mit",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.10"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "goldinteractive\\sitesfield\\": "src/"
        }
    },
    "extra": {
        "handle": "sites-field",
        "name": "Sites Field",
        "developer": "Goldinteractive",
        "documentationUrl": ""
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

